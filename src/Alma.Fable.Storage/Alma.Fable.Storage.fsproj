<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <OutputType>Library</OutputType>
        <PackageId>Alma.Fable.Storage</PackageId>
        <Version>5.0.0</Version>
        <PackageProjectUrl>https://github.com/almacareer/prc-fable-storage</PackageProjectUrl>
        <RepositoryUrl>https://github.com/almacareer/prc-fable-storage</RepositoryUrl>
    </PropertyGroup>

    <ItemGroup>
        <Compile Include="AssemblyInfo.fs" />
    </ItemGroup>

    <ItemGroup>
        <Compile Include="LocalStorage.fs" />
    </ItemGroup>

    <ItemGroup>
        <Content Include="*.fsproj; *.fs;" PackagePath="fable\" />
    </ItemGroup>

    <Import Project="..\..\.paket\Paket.Restore.targets" />

</Project>